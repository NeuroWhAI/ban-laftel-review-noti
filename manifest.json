{
  "name": "ban-laftel-review-noti",
  "description": "라프텔의 알림 화면에서 리뷰를 좋아하는 사람이 있다는 내용 삭제.",
  "version": "1.1",
  "manifest_version": 3,
  "permissions": ["tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://laftel.net/*"],
      "js": ["content-script.js"]
    }
  ],
  "action": {
    "default_icon": {
      "16": "/icon-16x16.png",
      "32": "/icon-32x32.png",
      "48": "/icon-48x48.png"
    }
  },
  "icons": {
    "16": "/icon-16x16.png",
    "32": "/icon-32x32.png",
    "48": "/icon-48x48.png"
  }
}
